<% provide(:title, 'All users') %>
<% provide(:search_name, 'user search') %>
<% provide(:path, users_path) %>
<h1>All users (<%= @users.count %>)</h1>
<div class="row">
  <div class="col-md-6 offset-md-3">
  <div class= "pagination justify-content-center">
    <%= will_paginate %>
  </div>
    <ul class="users">
      <%= render 'users/search' %>
      <% @users.each do |user| %>
        <%= render @users %>
      <% end %>
    </ul>
    <div class= "pagination justify-content-center">
    <%= will_paginate %>
  </div>
  </div>
</div>